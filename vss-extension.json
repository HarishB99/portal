{
    "manifestVersion": 1,
    "id": "harish-release-task-page",
    "version": "0.0.0.3",
    "name": "Harish Release Page",
    "description": "A Visual Studio Services contribution extension for Harish Release Tools.",
    "publisher": "HarishB",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Build and release"
    ],
    "contributions": [
        {
            "id": "harish-approval-release-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-release-hub-group"
            ],
            "properties": {
                "name": "Approvals",
                "order": 1,
                "uri": "index.html"
            }
        }
    ],
    "files": [
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true
        }
    ]
}